        "@types/lodash": "^4.17.13",
        "@types/write-file-atomic": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "better-sqlite3": "^7.4.5",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^29.3.1",
        "mongoose": "^8.9.2",
        "mysql2": "^3.2.0",
        "prettier": "^2.5.1",
        "ts-jest": "^29.0.3",
        "typedoc": "^0.23.26",
        "typescript": "^4.5.4",
        "write-file-atomic": "^6.0.0"
